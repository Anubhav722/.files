{
  "session_name": "appknox",
  "config": "~/.files/tmux.conf",
  "windows": [
    {
      "window_name": "dbs",
      "layout": "tiled",
      "panes": [
          "redis-server",
          "pg_ctl -D /usr/local/var/postgres start",
          "rabbitmq-server",
          "minio server ~/Projects/minio"
      ]
    },
    {
      "window_name": "mycroft",
      "shell_command_before": "mycroft",
      "layout": "tiled",
      "panes": [
          "echo \"./manage.py runserver\"",
          "echo \"./scripts/dev_celery.sh\"",
          "g pl",
          "echo \"./scripts/socket.sh\""
      ]
    },
    {
      "window_name": "sherlock",
      "shell_command_before": "sherlock",
      "layout": "tiled",
      "panes": [
          "./manage.py runserver",
          "./scripts/dev_celery.sh",
          "g pl",
          "./scripts/socket.sh"
      ]
    },
    {
      "window_name": "moriarty",
      "layout": "tiled",
      "shell_command_before": "moriarty",
      "panes": [
          "echo \"./manage.py runserver 0.0.0.0:8888\"",
          ".echo \"/manage.py devices\"",
          "g pl",
          "echo \"./manage.py websockify\""
      ]
    },
    {
      "window_name": "irene",
      "shell_command_before": "irene",
      "layout": "tiled",
      "panes": [
          "g pl",
          "ember serve"
      ]
    },
    {
      "window_name": "commands",
      "panes": [
        "time"
      ]
    }
  ]
}
